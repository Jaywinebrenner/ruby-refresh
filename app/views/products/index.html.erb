<h1>Products Page</h1>

<%= link_to "New Product", products_new_path %>

<% @products.each do |product| %>
    <br>
    <%= product.id %>
        <p><%= product.title %> </p>
        <p><%= product.price %> </p>
        <p><%= product.description %> </p>
        <p><%= product.imageSource %> </p>
    <br> 
<% end %>